= form_with model: @artist do |f|
  = f.label :name
  = f.text_field :name
  %br/

  = f.label :permalink
  = f.text_field :permalink
  
  %br/
  = f.label :bio
  = f.text_area :bio

  %br/

  = f.label :formed_at
  = f.date_field :formed_at

  %br/

  = f.label :verified_at
  = f.date_field :verified_at

  %br/

  = f.label :avatar_url
  = f.url_field :avatar_url


  %br/

  = f.label :cover_photo_url
  = f.url_field :cover_photo_url

  %br/

  = f.submit
  
  %br/
  = link_to "Back to Artists", artists_path